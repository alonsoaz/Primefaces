<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">
	
  	<application>
    	<el-resolver>
    		org.springframework.web.jsf.el.SpringBeanFacesELResolver
    	</el-resolver>
  	</application>
    
 <navigation-rule> 
    
    <from-view-id>*</from-view-id> 
      <!-- index -->
    <navigation-case> 
      <from-outcome>index</from-outcome> 
      <to-view-id>index.xhtml</to-view-id> 
    </navigation-case> 
    
     <!-- Panel -->
    <navigation-case> 
      <from-outcome>panel</from-outcome> 
      <to-view-id>inicio/panel.xhtml</to-view-id> 
    </navigation-case> 
    
    <!-- Alumno -->
    <navigation-case> 
      <from-outcome>alumno_listado</from-outcome> 
      <to-view-id>alumno_listado.xhtml</to-view-id> 
    </navigation-case> 
    
     <navigation-case> 
      <from-outcome>alumno_registro</from-outcome> 
      <to-view-id>alumno_registro.xhtml</to-view-id> 
    </navigation-case> 

 </navigation-rule>
 
 

</faces-config>
